using System;

class Program
{
    static void Main(string[] args)
    {
        // Declaración de variables
        int edad = 25;
        string nombre = "Luis";
        bool tieneLicencia = true;

        // Condición 1: Edad
        if (edad >= 18)
        {
            Console.WriteLine(nombre " puede conducir");
        }
        else
        {
            Console.WriteLine(nombre " no puede conducir");
        }
        // Condición 2: Licencia
        if (tieneLicencia == true)
        {
            Console.WriteLine(nombre " tiene licencia");
        }
        else
        {
            Console.WriteLine(nombre " no tiene licencia");
        }
    }
}
